{
    "name": "wyrihaximus/metrics",
    "description": "\ud83d\udd75\ufe0f\u200d\u2640\ufe0f Prometheus modelled metric VO's and registry",
    "license": "MIT",
    "authors": [
        {
            "name": "Cees-Jan Kiewiet",
            "email": "ceesjank@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "lcobucci/clock": "^2.0",
        "react-parallel/object-proxy-attributes": "^1",
        "thecodingmachine/safe": "^1.1",
        "wyrihaximus/constants": "^1.5"
    },
    "require-dev": {
        "phpbench/phpbench": "^0.17.1 || ^1.0.0",
        "wyrihaximus/test-utilities": "^2.5 || ^3.0"
    },
    "config": {
        "platform": {
            "php": "7.4.7"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "WyriHaximus\\Metrics\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WyriHaximus\\Tests\\Metrics\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "composer normalize"
        ],
        "post-update-cmd": [
            "composer normalize"
        ]
    }
}
